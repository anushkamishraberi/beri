mat1 = [[1, 2],
        [5, 3]]

mat2 = [[2, 3],
        [4, 1]]
result = [
    [
        mat1[0][0]*mat2[0][0] + mat1[0][1]*mat2[1][0],
        mat1[0][0]*mat2[0][1] + mat1[0][1]*mat2[1][1]
    ],
    [
        mat1[1][0]*mat2[0][0] + mat1[1][1]*mat2[1][0],
        mat1[1][0]*mat2[0][1] + mat1[1][1]*mat2[1][1]
    ]
]
print("Product of mat1 and mat2:")
for row in result:
    print(row)
